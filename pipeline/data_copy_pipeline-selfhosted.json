{
	"name": "data_copy_pipeline-selfhosted",
	"properties": {
		"activities": [
			{
				"name": "CopyDataFromOnPremToAzure",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": "select top 100 BusinessEntityID,\nTitle,\nFirstName,\nMiddleName,\nLastName from person.person",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings",
							"copyBehavior": "FlattenHierarchy"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false,
					"enableSkipIncompatibleRow": true,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "BusinessEntityID",
									"type": "Int32"
								},
								"sink": {
									"name": "BusinessEntityID",
									"type": "Int32"
								}
							},
							{
								"source": {
									"name": "Title",
									"type": "String"
								},
								"sink": {
									"name": "Title",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "FirstName",
									"type": "String"
								},
								"sink": {
									"name": "FirstName",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "MiddleName",
									"type": "String"
								},
								"sink": {
									"name": "MiddleName",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "LastName",
									"type": "String"
								},
								"sink": {
									"name": "LastName",
									"type": "String"
								}
							}
						]
					}
				},
				"outputs": [
					{
						"referenceName": "ADLS_Parquet",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}