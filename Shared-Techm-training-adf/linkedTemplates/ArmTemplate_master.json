{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Shared-Techm-training-adf"
		},
		"ADF_storage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ADF_storage'"
		},
		"ADLS_NAVEEN_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_NAVEEN'"
		},
		"RDBMS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'RDBMS'"
		},
		"sqlserver_dev_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sqlserver_dev'"
		},
		"ADLS_NAVEEN_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://naveensqldbstorage.dfs.core.windows.net/"
		},
		"AZ_KV_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://naveen-dev-keyvault-2023.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Shared-Techm-training-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADF_storage_connectionString": {
						"value": "[parameters('ADF_storage_connectionString')]"
					},
					"ADLS_NAVEEN_accountKey": {
						"value": "[parameters('ADLS_NAVEEN_accountKey')]"
					},
					"RDBMS_connectionString": {
						"value": "[parameters('RDBMS_connectionString')]"
					},
					"sqlserver_dev_connectionString": {
						"value": "[parameters('sqlserver_dev_connectionString')]"
					},
					"ADLS_NAVEEN_properties_typeProperties_url": {
						"value": "[parameters('ADLS_NAVEEN_properties_typeProperties_url')]"
					},
					"AZ_KV_properties_typeProperties_baseUrl": {
						"value": "[parameters('AZ_KV_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "Shared-Techm-training-adf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'Shared-Techm-training-adf_ArmTemplate_0')]"
			]
		}
	]
}